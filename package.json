{
  "name": "uxbit",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/uxbit/uxbit.esm.js",
  "exports": {
    ".": {
      "import": "./dist/uxbit/uxbit.esm.js",
      "require": "./dist/uxbit/uxbit.cjs.js"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stenciljs/component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "pnpm run build && storybook dev -p 6006 --no-open",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,css,scss,md,json,html}\"",
    "lint-staged": "lint-staged",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,css,scss,md,json,html}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@stencil/core": "^4.38.3",
    "@stencil/storybook-plugin": "^0.5.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "puppeteer": "^24.3.0",
    "storybook": "9.1.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4"
  },
  "license": "MIT"
}
