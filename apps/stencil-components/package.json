{
  "name": "@uxbit/stencil-components",
  "version": "0.0.1",
  "description": "Reusable UXBIT web components built with Stencil.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/uxbit/uxbit.esm.js",
  "exports": {
    ".": {
      "import": "./dist/uxbit/uxbit.esm.js",
      "require": "./dist/uxbit/uxbit.cjs.js"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    },
    "./custom-elements.json": {
      "default": "./custom-elements.json"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stenciljs/component-starter.git"
  },
  "files": [
    "dist/",
    "loader/",
    "custom-elements.json",
    "custom-elements.d.ts"
  ],
  "scripts": {
    "dev": "stencil build --watch",
    "build": "stencil build",
    "serve": "stencil build --watch --serve",
    "start": "pnpm run serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,css,scss,md,json,html}\"",
    "generate:component": "node scripts/generate-component.cjs",
    "generate:image": "node server/section-to-image.mjs",
    "generate:mp4": "sh -c 'node ./server/section-to-mp4.mjs \"$1\" \"$2\" 60' --"
  },
  "devDependencies": {
    "@stencil/core": "^4.38.3",
    "@stencil/storybook-plugin": "^0.5.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "@types/react": "18.3.12",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "prettier": "^3.6.2",
    "puppeteer": "^24.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4"
  },
  "license": "MIT",
  "dependencies": {
    "node-html-to-image": "^5.0.0"
  }
}
